    "@types/pg": "^8.16.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@types/tsscmp": "^1.0.2",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "react-email": "5.2.5",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
