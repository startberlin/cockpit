{
  "name": "start-cockpit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run inngest:dev\" \"npm run next:dev\"",
    "next:dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:up": "docker compose up -d",
    "db:seed": "dotenv -e .env -- tsx scripts/seed.ts",
    "db:dump": "dotenv -e .env -- sh -c 'pg_dump \"$DATABASE_URL\" --format=plain --no-owner --no-privileges --clean --if-exists --file supabase.sql'",
    "db:restore": "dotenv -e .env -- sh -c 'psql \"$DATABASE_URL\" < supabase.sql'",
    "google:add-member": "dotenv -e .env -- tsx scripts/google.ts",
    "email:dev": "email dev --dir src/emails",
    "inngest:dev": "npx inngest-cli@latest dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next-safe-action/adapter-react-hook-form": "^2.0.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "1.0.6",
    "@react-email/render": "^2.0.4",
    "@slack/web-api": "^7.13.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/og": "^0.8.6",
    "bcryptjs": "^3.0.3",
    "better-all": "^0.0.7",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.8.3",
    "google-auth-library": "^10.5.0",
    "googleapis": "^171.0.0",
    "inngest": "^3.50.0",
    "libphonenumber-js": "^1.12.36",
    "lodash-es": "^4.17.23",
    "lodash.merge": "^4.6.2",
    "lucide-react": "^0.563.0",
    "motion": "^12.29.2",
    "next": "16.1.6",
    "next-safe-action": "^8.0.11",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.7",
    "pg": "^8.18.0",
    "postcss": "^8.5.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "resend": "^6.9.1",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
